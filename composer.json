{
    "name": "dadangnh/iam",
    "description": "Identity and Access Management System with API Platform",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "keywords": ["iam","identity and access management","api","api platform","symfony","symfony iam","rbac"],
    "homepage": "https://gitlab.com/dadangnh/iam",
    "authors": [
        {
            "name": "Dadang NH",
            "email": "project@dadangnh.com",
            "homepage": "https://www.dadangnh.com",
            "role": "Developer"
        },
        {
            "name": "Hariyadi P",
            "email": "sasta.hp@gmail.com",
            "role": "Developer"
        },
        {
            "name": "IAM Contributor",
            "homepage": "https://gitlab.com/dadangnh/iam/-/graphs/master"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "api-platform/core": "^2.6",
        "composer/package-versions-deprecated": "^1.11.99.4",
        "doctrine/annotations": "^1.13",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "easycorp/easyadmin-bundle": "^4.0",
        "gesdinet/jwt-refresh-token-bundle": "^1.1",
        "lexik/jwt-authentication-bundle": "^2.14",
        "nelmio/cors-bundle": "^1.5 || ^2.0",
        "phpdocumentor/reflection-docblock": "^3.0 || ^4.0 || ^5.0",
        "phpstan/phpdoc-parser": "^1.2",
        "predis/predis": "^1.1",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/asset": "6.2.*",
        "symfony/cache": "6.2.*",
        "symfony/console": "6.2.*",
        "symfony/css-selector": "^6.2",
        "symfony/dotenv": "6.2.*",
        "symfony/expression-language": "6.2.*",
        "symfony/flex": "^2.0",
        "symfony/framework-bundle": "6.2.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/phpunit-bridge": "^6.2",
        "symfony/property-access": "6.2.*",
        "symfony/property-info": "6.2.*",
        "symfony/proxy-manager-bridge": "6.2.*",
        "symfony/runtime": "6.2.*",
        "symfony/security-bundle": "6.2.*",
        "symfony/serializer": "6.2.*",
        "symfony/twig-bundle": "6.2.*",
        "symfony/uid": "6.2.*",
        "symfony/validator": "6.2.*",
        "symfony/yaml": "6.2.*",
        "webonyx/graphql-php": "^14.11"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "6.2.*",
        "symfony/debug-bundle": "^6.2",
        "symfony/http-client": "6.2.*",
        "symfony/maker-bundle": "^1.36",
        "symfony/stopwatch": "^6.2",
        "symfony/var-dumper": "^6.2",
        "symfony/web-profiler-bundle": "^6.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.2.*",
            "docker": true
        }
    }
}
